      steps {
        echo 'building dell applicazione...'
        echo 'modificato'
      }
    }
    
    stage("test") {
      
      steps {
        echo 'test dell applicazione...'
      }
    }
    
    stage("deploy") {
      
      steps {
        echo 'deploy dell applicazione...'
      }
    }
  }
}
